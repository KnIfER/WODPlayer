<?xml version="1.0" encoding="utf-8"?>
<Window caption="0,0,0,30"  inset="0,0,0,0" roundcorner="5,5,5,5"  inset="10,10,10,10" sizebox="4,4,4,4" showdirty="false" layered="false">
 

	<style>
		 <Button
			id="btn_style"
			apply="true"
			shared="true"
			hotbkcolor="#2e008CFF" 
			pushedbkcolor="#34008CFF" 
			hotbordercolor="#34008CFF" 
			pushedbordercolor="#67008CFF" 
			bordersize="1"
		 />
	</style>
 
 
<VBox bkcolor="#FFFFFFFF"
	clipchild="false" 
	vscrollbar="true"
	name="top"
	>

	
	<_v height="0" bkcolor="#FF555500" name="ch0"/>
	
			
	<WinFrame height="-2" name="ch1">
		<VBox bkcolor="#FF000000"
			height="-2"
			>
			<HBox bkcolor="#FF000000"
				height="35"
				>
				<_s inset="0,10,0,10" minwidth="100" width="0" name="seekbar" barInset="10"/>
				<_s inset="0,10,0,10" width="100"  bkcolor="#FF000000"
					barInset="10"/>
			</HBox>
			
			<_t height="50" name="btns"/>
			
			<HBox  height="48"
				>
				<Button width="48" name="play" foreimage="play.png" />
				<Button width="48" name="stop" foreimage="stop.png"/>
				<Button width="48" name="prev" foreimage="prev.png"/>
				<Button width="48" name="nxt" foreimage="nxt.png"/>
				<Button width="48" name="folder" foreimage="folder.png"/>
			</HBox>
			
			<!--btnlst height="-2"/-->
		</VBox>
	</WinFrame>

</VBox>



</Window>